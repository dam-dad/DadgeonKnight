<fx:root fx:id="view" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" stylesheets="@../css/settings.css" type="VBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"> 
   <TableView fx:id="leadersTavbleView">
        <columns>
            <TableColumn fx:id="userTableCollumn" maxWidth="1.7976931348623157E308" prefWidth="200.0" text="UserName">
                <cellValueFactory>
                    <PropertyValueFactory property="twitterHandle" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="timeTableCollumn" maxWidth="1.7976931348623157E308" prefWidth="100.0" text="Time">
                <cellValueFactory>
